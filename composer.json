{
    "name": "ezsystems/payment-paypal-bundle",
    "type": "symfony-bundle",
    "description": "Payment Bundle providing access to the PayPal API",
    "keywords": ["payment", "paypal"],
    "homepage": "http://jmsyst.com/bundles/JMSPaymentPaypalBundle",
    "license": "Apache2",
    "replace": {
        "jms/payment-paypal-bundle": "*"
    },
    "authors": [
        {
            "name": "Johannes M. Schmitt",
            "email": "schmittjoh@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "symfony/browser-kit": ">=5.1 <6.0",
        "symfony/config": ">=5.1 <6.0",
        "symfony/dependency-injection": ">=5.1 <6.0",
        "symfony/form": ">=5.1 <6.0",
        "symfony/http-foundation": ">=5.1 <6.0",
        "symfony/http-kernel": ">=5.1 <6.0",
        "ezsystems/payment-core-bundle": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8|~5.4",
        "symfony/phpunit-bridge": "^5.1"
    },
    "autoload": {
        "psr-0": { "JMS\\Payment\\PaypalBundle": "" }
    },
    "target-dir": "JMS/Payment/PaypalBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
